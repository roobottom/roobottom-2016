{% macro m_postNote(post) %}
  <article class="m_postNote">
    {% if post.attributes.images %}
      <div class="m_postNote__gallery">
        {{ m_gallery(post.attributes.images,'notes') }}
      </div>
    {% endif %}
    <div class="m_postNote__body type">
      {{post.html_body}}
    </div>
  </article>
{% endmacro %}
